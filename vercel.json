{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/404.js" }
  ],
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }