{
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ],
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      }
    ],
    "routes": [
        { "handle": "filesystem" ,
          "src": "/(.*)", "status": 404, "dest": "/*" ,
        "dest": "server.js"
      }
    ]
  }
